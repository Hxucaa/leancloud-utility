{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src/"],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "systemName"      : "Validation Stack",
    "footer"          : "",
    "copyright"       : "Copyright Â© 2016 ZenChat Interactive Inc. All Rights Reserved.",
    "navType"         : "vertical",
    "theme"           : "cerulean",
    "linenums"        : true,
    "collapseSymbols" : false,
    "inverseNav"      : true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./jsdoc/",
    "recurse": true,
    "private": true,
    "readme": "./README.md"
  }
}
